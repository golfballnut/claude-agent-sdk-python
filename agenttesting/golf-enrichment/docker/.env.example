# Golf Enrichment V2 Validator - Docker Test Environment
# Copy this file to .env and fill in your credentials

# ============================================================
# REQUIRED: Supabase Configuration
# ============================================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here

# ============================================================
# OPTIONAL: Contact Enrichment APIs
# (Only needed if testing enrichment waterfall)
# ============================================================
APOLLO_API_KEY=
HUNTER_API_KEY=
CLICKUP_API_TOKEN=

# ============================================================
# OPTIONAL: Testing Configuration
# ============================================================
# Set to true to skip database verification (faster tests)
SKIP_DB_VERIFICATION=false

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
