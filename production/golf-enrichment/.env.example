# Environment Variables for Full Orchestrator Deployment
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git!

# Required: Perplexity API for web search (Agents 5, 6, 6.5, 7)
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Required: Anthropic API for Claude Code CLI
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Required: Apollo.io for contact discovery + email enrichment (Agent 2-Apollo)
# Plan: Professional ($79/month, 4,020 credits)
# Replaces: Agents 2, 3, 4 (discovery, email, LinkedIn)
APOLLO_API_KEY=your_apollo_api_key_here

# Optional: Hunter.io for email finding (backup/fallback if needed)
# HUNTER_API_KEY=your_hunter_api_key_here

# Required: Supabase for data storage (Agent 8)
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Optional: Web scraping (Agent 2)
FIRECRAWL_API_KEY=your_firecrawl_api_key_here
JINA_API_KEY=your_jina_api_key_here
BRIGHTDATA_API_TOKEN=your_brightdata_token_here

# Optional: Logging configuration
LOG_LEVEL=INFO

# Optional: Environment identifier
ENVIRONMENT=development
